<!DOCTYEP html>
<html>
  <head>
    <meta charset="utf-8"/>
  </head>
    <body>
      <div id = "login" >
        <h2>My User</h2>
        <% if flash[:notice] %>
          <%= flash[:notice] %>
        <% end %>
        <% @users.each do |e| %>
          <p>
            <a>ユーザー名：<%= e.uid %></a><br>
            <a>パスワード：<%= e.pass %></a><br>
            <%= link_to '削除', user_path(e), method: :delete, class:"link3" %>
          </p>
        <% end %>
      </div>
    </body>
  </html>
